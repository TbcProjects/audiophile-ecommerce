  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "compile": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "watch": "graphql-codegen -w",
