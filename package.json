    "@emotion/styled": "^11.10.5",
    "@fontsource/manrope": "^4.5.13",
    "@types/chec__commerce.js": "^2.8.6",
    "@types/node": "18.15.11",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "eslint": "8.38.0",
